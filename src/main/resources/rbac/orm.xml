<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.bobdeng.rbac">
    <class name="Tenant" table="t_rbac_tenant">
        <id name="id" type="int" column="ID">
            <generator class="native"/>
        </id>
        <component name="description" class="TenantDescription">
            <property name="name" column="name"/>
        </component>

    </class>
    <class name="User" table="t_rbac_user">
        <id name="id" type="int" column="id">
            <generator class="native"></generator>
        </id>
        <component name="description" class="UserDescription">
            <property name="name" column="name"/>
        </component>
    </class>
</hibernate-mapping>